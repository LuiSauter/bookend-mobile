{
  "expo": {
    "name": "Bookend",
    "slug": "bookend",
    "version": "1.0.0",
    "orientation": "portrait",
    "jsEngine": "hermes",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/img/bookend.png",
      "resizeMode": "contain",
      "backgroundColor": "#192734"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "scheme": "com.sauter.bookend",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sauter.bookend",
      "buildNumber": "3"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#192734"
      },
      "package": "com.sauter.bookend",
      "versionCode": 1
    },
    "androidStatusBar": {
      "translucent": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
